pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- variables for animation
local player = {
    x = 64,          -- x position
    y = 64,          -- y position
    speed = 1,       -- movement speed
    anim_frames = {1, 2, 3, 4, 5, 6, 7, 8}, -- sprite indices for walk cycle
    anim_index = 1,  -- current frame in animation
    anim_timer = 0,  -- timer to control animation speed
    anim_speed = 0.4, -- time between frames
    flip_x = false   -- flip horizontally
}

function _update()
    -- movement input
    local dx, dy = 0, 0
    if btn(0) then 
        dx = -1 
        player.flip_x = true  -- flip sprite for left
    end
    if btn(1) then 
        dx = 1  
        player.flip_x = false -- no flip for right
    end
    if btn(2) then dy = -1 end -- up
    if btn(3) then dy = 1 end  -- down

    -- update player position
    player.x += dx * player.speed
    player.y += dy * player.speed

    -- update animation if moving
    if dx ~= 0 or dy ~= 0 then
        player.anim_timer += player.anim_speed
        if player.anim_timer >= 1 then
            player.anim_timer = 0
            player.anim_index = (player.anim_index % #player.anim_frames) + 1

            -- play sound on frames 3 and 7
            local frame = player.anim_frames[player.anim_index]
            if frame == 3 or frame == 7 then
                sfx(3)
            end
        end
    end
end

function _draw()
    -- clear screen
    cls()

    -- draw player with current animation frame
    local sprite = player.anim_frames[player.anim_index]
    spr(sprite, player.x, player.y, 1, 1, player.flip_x)
end

